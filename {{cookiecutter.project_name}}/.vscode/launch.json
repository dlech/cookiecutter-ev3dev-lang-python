{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Download and Run main.py",
      "type": "ev3devBrowser",
      "request": "launch",
      "program": "/home/robot/${workspaceFolderBasename}/main.py",
      "interactiveTerminal": true
